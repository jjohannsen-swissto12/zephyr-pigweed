    "files.associations": {
        "*.conf": "bitbake",
        "*.inc": "bitbake",
        "regex": "c",
        "gpio.h": "c",
        "device.h": "c",
        "ratio": "c",
        "system_error": "c",
        "array": "c",
        "tuple": "c",
        "type_traits": "c",
        "utility": "c",
        "limits": "c",
        "functional": "c",
        "app_shared.h": "c",
        "kernel.h": "c",
        "sys_io.h": "c",
        "stacktrace": "c",
        "app_memdomain.h": "c",
        "devicetree.h": "c",
        "kernel_structs.h": "c",
        "locale": "c",
        "kernel_includes.h": "c",
        "libc-hooks.h": "c",
        "sample_driver.h": "c"
    },
    "terminal.integrated.defaultProfile.linux": "Zephyr IDE Terminal",
    "cmake.configureOnOpen": false,
    "zephyr-ide.use-zephyr-ide-json": true,
    "zephyr-ide.projects": null
